{
  "$schema": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json",
  "configurations": {
    "main": {
      "adapter": "debugpy",
      "configuration": {
        "name": "run the executable",
        "cwd": "${workspaceRoot}/src",
        "type": "python",
        "request": "launch",
        "python.pythonPath": "/usr/bin/env python3",
        "module": "linkspocket",
        "args": [
          "-r",
          "~/zootr/rom.z64"
        ]
      }
    }
  }
}
